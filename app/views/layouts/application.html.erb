    <title>First app</title>
     <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
  </head>
  <body>
  <%= render 'home/header' %>
  <div class= "container">
  <br/>
    <%= yield %>
  </div>
     

